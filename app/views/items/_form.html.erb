
<% if user_signed_in?%>
	<%= simple_form_for @item do |f| %>
	  <%= f.input :title, label: "Item" %>
	  <%= f.input :description %>
	  <%= f.input :category, :collection => ['January','February','December'] %>
	  <%= f.button :submit %>
	<% end %>

<%else%>

	<%= render 'layouts/static_home_page'%>
<%end%> 